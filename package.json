{
  "name": "multi-page-agg",
  "description": "使用github action尝试将一个repo下的多个项目聚合部署在同一个页面",
  "type": "module",
  "version": "0.0.2",
  "author": "",
  "private": true,
  "homepage": "https://github.com/RSS1102/multi-page-agg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RSS1102/multi-page-agg.git"
  },
  "bugs": {
    "url": "https://github.com/RSS1102/multi-page-agg/issues"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "farmup --format esm src/index.ts --no-exec",
    "release": "npm run build && npm version patch && npm publish"
  },
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gh-pages": "^6.1.0",
    "@types/node": "^20.12.12",
    "make-coverage-badge": "^1.2.0",
    "typescript": "^5.4.5"
  }
}
